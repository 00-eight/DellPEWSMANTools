{
    "support" : {
        "resourceUriBase" : "http://schemas.dmtf.org/wbem/wscim/1/cim-schema/2/",
        "namespace" : "root/dcim",
        "IgnoreList" : [
            "PSComputerName",
            "InstanceID",
            "CIM*"
        ],
        "DriveFormFactor" : {
            "0" : "Unknown",
            "1" : "1.8-inch",
            "2" : "2.5-inch",
            "3" : "3.5-inch",
            "4" : "2.5 inch Add-in card"
        },
        "MediaType" : {
            "0" : "HDD",
            "1" : "SSD"
        }
    },
    "data" : {
        "BIOS" : {
            "class" : "DCIM_PhysicalComputerSystemView",
            "selectorSet" : [
                "CurrentBIOSVersionString"
            ],
            "filter" : "",
            "validateSet" : []            
        },
        "Memory" : {
            "class" : "DCIM_MemoryView",
            "selectorSet" : [
                "Size"
            ],
            "filter" : "",
            "validateSet" : []            
        }, 
        "CPU" : {
            "class" : "DCIM_CPUView",
            "selectorSet" : [
                "Model"
            ],
            "filter" : "",
            "validateSet" : []            
        },
        "CPLD" : {
            "class" : "DCIM_SoftwareIdentity",
            "selectorSet" : [
                "VersionString"
            ],
            "filter" : "ElementName='System CPLD'",
            "validateSet" : []            
        },            
        "DRAC" : {
            "class" : "DCIM_PhysicalComputerSystemView",
            "selectorSet" : [
                "CurrentManagementFirmwareVersionString"
            ],
            "filter" : "",
            "validateSet" : []  
        },
        "Network" : {
            "class" : "DCIM_NICView",
            "selectorSet" : [
                "ProductName",
                "MediaType",
                "PermanentMACAddress",
                "VendorName",
                "FamilyVersion",
                "NewProductName"
            ],
            "filter" : "",
            "validateSet" : []
        },        
        "RAID" : {
            "class" : "DCIM_ControllerView",
            "selectorSet" : [
                "ProductName",
                "DeviceDescription",
                "ControllerFirmwareVersion"
            ],
            "filter" : "",
            "validateSet" : []
        },        
        "HDD" : {
            "class" : "DCIM_PhysicalDiskView",
            "selectorSet" : [
                "Manufacturer",
                "Model",
                "Revision",
                "SerialNumber",
                "SizeInBytes",
                "MediaType",
                "DriveFormFactor"
            ],
            "filter" : "MediaType=0",
            "validateSet" : []
        },
        "SSD" : {
            "class" : "DCIM_PhysicalDiskView",
            "selectorSet" : [
                "Manufacturer",
                "Model",
                "Revision",
                "SerialNumber",
                "SizeInBytes",
                "MediaType",
                "DriveFormFactor"
            ],
            "filter" : "MediaType=1",
            "validateSet" : []
        },
        "PCIe" : {
            "class" : "DCIM_PCIeSSDView",
            "selectorSet" : [
                "Manufacturer",
                "Model",
                "Revision",
                "SerialNumber",
                "SizeInBytes",
                "MediaType",
                "DriveFormFactor"
            ],
            "filter" : "MediaType=1",
            "validateSet" : []
        },                 
        "Software" : {
            "class" : "DCIM_SoftwareIdentity",
            "selectorSet" : [
                "FQDD",
                "ElementName",
                "VersionString"
            ],
            "filter" : "Status='Installed'",
            "validateSet" : []       
        },
        "BootMode" : {
            "class" : "DCIM_BIOSEnumeration",
            "selectorSet" : [
                "AttributeName",
                "CurrentValue"
            ],
            "filter" : "GroupID='BootSettings' AND AttributeName='BootMode'",
            "validateSet" : [
                {
                    "AttributeName": "BootMode",
                    "AdditionalProperties" : [
                        {
                            "CurrentValue" : "UEFI"
                        }
                    ]
                }               
            ]
        },
        "BIOSAttributes" : {
            "class" : "DCIM_BIOSEnumeration",
            "selectorSet" : [
                "AttributeName",
                "CurrentValue"
            ],
            "filter" : "(GroupID='ProcSettings' OR GroupID='SysProfileSettings') AND ((AttributeName='LogicalProc' OR AttributeName='ProcVirtualization' OR AttributeName='ProcExecuteDisable') OR (AttributeName='SysProfile' OR AttributeName='ProcPwrPerf' OR AttributeName='ProcCStates'))",
            "validateSet" : [
                {
                    "AttributeName": "LogicalProc",
                    "AdditionalProperties" : [
                        {
                            "CurrentValue" : "Enabled"
                        }
                    ]
                },
                {
                    "AttributeName": "ProcVirtualization",
                    "AdditionalProperties" : [
                        {
                            "CurrentValue" : "Enabled"
                        }
                    ]
                },
                {
                    "AttributeName": "ProcExecuteDisable",
                    "AdditionalProperties" : [
                        {
                            "CurrentValue" : "Enabled"
                        }
                    ]
                },
                {
                    "AttributeName": "SysProfile",
                    "AdditionalProperties" : [
                        {
                            "CurrentValue" : "PerfOptimized"
                        }
                    ]
                },
                {
                    "AttributeName": "ProcPwrPerf",
                    "AdditionalProperties" : [
                        {
                            "CurrentValue" : "MaxPerf"
                        }
                    ]
                },
                {
                    "AttributeName": "ProcCStates",
                    "AdditionalProperties" : [
                        {
                            "CurrentValue" : "Disabled"
                        }
                    ]
                }                                                        
            ]         
        }
    }
}
